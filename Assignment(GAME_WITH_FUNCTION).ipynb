    "#write a python function to sum all the numbers in a list\n",
    "\n",
    "def sum_list():\n",
    "    numbers=input(\"enter a list,(separate by space):\").split()\n",
    "    total = 0\n",
    "    for num in numbers:\n",
    "        if num.isdigit():\n",
    "            total += int(num)\n",
    "    return total\n",
    "\n",
    "\n",
    "result = sum_list()\n",
    "print(result)"
   ]
